{
	"name": "rise-vest-senior-backend-challenge",
	"version": "1.0.0",
	"description": "rise vest technical assessment for senior backend engineer",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"clean": "rm -fr dist",
		"lint:fix": "eslint . --ext .ts --fix",
		"start": "typeorm:migration:run dist/index.js",
		"dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
		"build": "tsc",
		"typeorm:migration:generate": "npm run typeorm -- migration:generate src/migrations/CreateTables",
		"typeorm:migration:run": "npm run typeorm -- migration:run",
		"typeorm:migration:revert": "npm run typeorm -- migration:revert",
		"typeorm": "TS_NODE_BASEURL=./dist ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/typeorm.config.ts",
		"postinstall": "npm run build"
	},
	"keywords": [
		"rise-vest",
		"backend",
		"challenge",
		"senior"
	],
	"author": "Emmanuel Abraham <a.emmanuel2@yahoo.com>",
	"license": "ISC",
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/morgan": "^1.9.3",
		"@types/node": "^16.7.2",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.1",
		"tsc-watch": "^4.5.0",
		"typescript": "^4.3.5",
		"@types/jest": "^29.5.4",
		"@types/method-override": "^0.0.32",
		"@types/pg": "^8.10.2",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"envalid": "^7.2.2",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"joi": "^17.4.2",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"morgan": "^1.10.0",
		"pg": "^8.11.3",
		"sequelize": "^6.37.1",
		"sequelize-typescript": "^2.1.6",
		"typeorm": "^0.3.17",
		"body-parser": "^1.20.2",
		"express-validator": "^7.0.1",
		"jest": "^29.6.4",
		"method-override": "^3.0.0",
		"tsconfig-paths": "^4.2.0"
	},
	"_moduleAliases": {
		"@/resources": "dist/resources",
		"@/utils": "dist/utils",
		"@/middleware": "dist/middleware",
		"@/config": "dist/config",
		"@/db": "dist/db",
		"@/migrations": "dist/migrations"
	}
}
